class MyStuff:
    x=360

p1 = MyStuff()
print(p1)
print(p1.x)

class Pawn:
    def __init__(pawn,name,age):
        pawn.name = name
        pawn.ae = age
        pawn.x=0
        pawn.y=0
    def __str__(pawn):
        return f"{pawn.name},Age:{pawn.ae},PositionX:{pawn.x},PositionY:{pawn.y}"

Cael = Pawn("Loverdale", 17)

print(Cael.name)
print(Cael.ae)

Cael.x=1
Cael.y=1

print(Cael)